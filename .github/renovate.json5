{
  '$schema': 'https://docs.renovatebot.com/renovate-schema.json',
  extends: ['config:base', 'group:allNonMajor'],
  labels: ['dependencies'],
  ignorePaths: ['**/__tests__/**'],
  dependencyDashboard: false,
  pin: false,
  rangeStrategy: 'bump',
  packageRules: [
    {
      depTypeList: ['peerDependencies'],
      enabled: false,
    },
    {
      packageNames: ['rxjs'],
      allowedVersions: '7.8.1',
    },
  ],
  ignoreDeps: [
    'node',
  ],
  schedule: ['on friday'],
}
